%gbml
  %locus #{@result[:locus]}
  %title #{@result[:title]}
  %definition #{@result[:definition]}
  %accession #{@result[:accession]}
  %version #{@result[:version]}
  %keywords #{@result[:keywords]}
  %soure #{@result[:source]}
  %organism #{@result[:organism]}
  %id #{@result[:id]}
  %sequence #{@result[:sequence]}
  -@annotations.each do |i|
    %annotation(id="#{i[:id]}" type="#{i[:type]}" caption="#{i[:caption]}" domain="#{i[:domain]}")
      -@annodata.each do |data|
        -if (data[:annotation] == i[:id])
          %data(id="#{data[:id]}" type="#{data[:infokey]}")
            #{data[:value]}
